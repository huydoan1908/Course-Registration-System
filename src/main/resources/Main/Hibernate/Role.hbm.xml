<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.Role" table="role" schema="manage student">
        <id name="roleId">
            <column name="role_id" sql-type="char(3)" length="3"/>
        </id>
        <property name="roleName">
            <column name="role_name" sql-type="char(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>