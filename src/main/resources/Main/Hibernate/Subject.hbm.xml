<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.Subject" table="subject" schema="manage student">
        <id name="subjectId">
            <column name="subject_id" sql-type="char(5)" length="5"/>
        </id>
        <property name="subjectName">
            <column name="subject_name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="credits">
            <column name="credits" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>