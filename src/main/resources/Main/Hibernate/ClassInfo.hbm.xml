<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.ClassInfo" table="class_info" schema="manage student">
        <id name="infoId">
            <column name="info_id" sql-type="int"/>
        </id>
        <property name="studentId">
            <column name="student_id" sql-type="char(8)" length="8" not-null="true"/>
        </property>
        <property name="classId">
            <column name="class_id" sql-type="char(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>