<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.Clazz" table="class" schema="manage student">
        <id name="classId">
            <column name="class_id" sql-type="char(8)" length="8"/>
        </id>
        <property name="total">
            <column name="total" sql-type="int" not-null="true"/>
        </property>
        <property name="male">
            <column name="male" sql-type="int" not-null="true"/>
        </property>
        <property name="female">
            <column name="female" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>