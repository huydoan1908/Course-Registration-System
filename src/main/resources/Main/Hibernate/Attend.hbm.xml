<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.Attend" table="attend" schema="manage student">
        <id name="attendId">
            <column name="attend_id" sql-type="int"/>
        </id>
        <property name="studentId">
            <column name="student_id" sql-type="char(8)" length="8" not-null="true"/>
        </property>
        <property name="subjId">
            <column name="subj_id" sql-type="char(5)" length="5" not-null="true"/>
        </property>
        <property name="semId">
            <column name="sem_id" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>