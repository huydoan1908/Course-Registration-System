<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.Session" table="session" schema="manage student">
        <id name="sessId">
            <column name="sess_id" sql-type="char(1)" length="1"/>
        </id>
        <property name="startTime">
            <column name="start_time" sql-type="time" not-null="true"/>
        </property>
        <property name="endTime">
            <column name="end_time" sql-type="time" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>