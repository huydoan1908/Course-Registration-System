<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Main.POJO.CourseRegister" table="course_register" schema="manage student">
        <id name="registId">
            <column name="regist_id" sql-type="int"/>
        </id>
        <property name="semId">
            <column name="sem_id" sql-type="int" not-null="true"/>
        </property>
        <property name="startDate">
            <column name="start_date" sql-type="date" not-null="true"/>
        </property>
        <property name="endDate">
            <column name="end_date" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>