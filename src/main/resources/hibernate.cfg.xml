<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="connection.url">jdbc:mysql://localhost:3306/manage student</property>
    <property name="connection.driver_class">com.mysql.cj.jdbc.Driver</property>


    <mapping class="Main.POJO.Clazz"/>
    <mapping resource="Main/Hibernate/Clazz.hbm.xml"/>
    <mapping class="Main.POJO.ClassInfo"/>
    <mapping resource="Main/Hibernate/ClassInfo.hbm.xml"/>
    <mapping class="Main.POJO.Course"/>
    <mapping resource="Main/Hibernate/Course.hbm.xml"/>
    <mapping class="Main.POJO.CourseRegister"/>
    <mapping resource="Main/Hibernate/CourseRegister.hbm.xml"/>
    <mapping class="Main.POJO.Role"/>
    <mapping resource="Main/Hibernate/Role.hbm.xml"/>
    <mapping class="Main.POJO.Semester"/>
    <mapping resource="Main/Hibernate/Semester.hbm.xml"/>
    <mapping class="Main.POJO.Session"/>
    <mapping resource="Main/Hibernate/Session.hbm.xml"/>
    <mapping class="Main.POJO.Subject"/>
    <mapping resource="Main/Hibernate/Subject.hbm.xml"/>
    <mapping class="Main.POJO.User"/>
    <mapping resource="Main/Hibernate/Attend.hbm.xml"/>
    <mapping class="Main.POJO.Attend"/>


  </session-factory>
</hibernate-configuration>